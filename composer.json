{
    "name": "mattyeend/guest-to-user-helper",
    "description": "Guest-to-user migration helper for Laravel.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Guest",
        "User",
        "Helper",
        "Guest-To-User-Helper",
        "Package"
    ],
    "authors": [
        {
            "name": "Matt Yeend",
            "email": "mail@mattyeend.co.uk"
        }
    ],
    "require": {
      "php": ">=8.2",
      "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
      "psr-4": {
        "MattYeend\\GuestToUserHelper\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "MattYeend\\GuestToUserHelper\\GuestUpgradeServiceProvider"
        ]
      }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
  }
  